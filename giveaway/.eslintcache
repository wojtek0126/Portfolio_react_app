ngth message 2000 chars\r\n            else if (messageInput.length > 2000) {\r\n                alert(\"wiadomość za długa, max 2000 znaków\");\r\n            }                                         \r\n            else{\r\n                alert(\"dane prawidłowe, wiadomość wysłana pomyślnie\");\r\n                //sending to API\r\n                sendMessage(newMessage);\r\n            }            \r\n        }\r\n        validateNameInput(nameData, emailData, messageData);           \r\n        // nullify state after submit   \r\n        nullifyState(setNameData);\r\n        nullifyState(setEmailData);\r\n        nullifyState(setMessageData);                 \r\n    }\r\n\r\n","C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\sendStep1\\SendStep1.js",[],"C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\Rhomb.js",[],"C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\StepsBase.js",["128"],"C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\sendStep3\\SendStep3.js",[],"C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\sendStep2\\SendStep2.js",[],"C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\sendStep4\\SendStep4.js",[],"C:\\CodersLab\\Portfolio_react_app\\giveaway\\src\\components\\sendItems\\sendSummary\\SendSummary.js",["129","130"],"import React, {useState} from 'react';\r\nimport { nextStep } from '../../functionsStorage/functions';\r\nimport bear from '../../../assets/images/bear.jpg'\r\nimport Icon1 from '../../../assets/icons/Icon-1.svg';\r\nimport Icon4 from '../../../assets/icons/Icon-4.svg';\r\n\r\nconst  SendSummary = () => {  \r\n    let [orderData, setOrderData] = useState([]);\r\n    const order = \r\n    {\r\n        itemName: localStorage.getItem(\"itemName\"),\r\n        bags: localStorage.getItem(\"bags\"),\r\n        receiver: localStorage.getItem(\"receiver\"),\r\n        localization: localStor